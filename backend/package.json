{
  "name": "terapiaconect-backend",
  "version": "1.0.0",
  "description": "Backend da plataforma TerapiaConect",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "setup": "npm install && npx prisma generate",
    "lint": "eslint src/**/*.js --fix",
    "init": "node init-project.js",
    "prebuild": "node src/convert-to-cjs.js",
    "build": "node build.js",
    "postbuild": "node dist/fix-routes.js",
    "copy-files": "copyfiles -u 0 \"prisma/**/*\" \"uploads/**/*\" \".env*\" dist",
    "build:js": "node build.js",
    "fix-routes": "node src/fix-routes.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "convert-to-cjs": "node src/convert-to-cjs.js",
    "vercel-build": "npm run build && npm run prisma:generate",
    "migrate": "npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "prepare-routes": "node scripts/rename-to-cjs.js"
  },
  "keywords": ["terapia", "saÃºde mental", "psicologia"],
  "author": "TerapiaConect",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.24.1",
    "path-to-regexp": "^6.2.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "esbuild": "^0.19.8",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "chalk": "^5.3.0",
    "glob": "^10.3.10"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "engines": {
    "node": ">=18"
  }
}
