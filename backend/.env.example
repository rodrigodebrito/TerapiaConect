# Configurações do servidor
NODE_ENV=development
PORT=3000

# Banco de dados
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/terapiaconect

# Chave secreta para JWT
JWT_SECRET=sua_chave_secreta_jwt
JWT_REFRESH_SECRET=sua_chave_secreta_refresh_jwt
JWT_EXPIRES_IN=1d
JWT_REFRESH_EXPIRES_IN=7d

# Configurações de CORS
CORS_ORIGIN=http://localhost:3001,http://localhost:5173

# URL da API (para uso interno)
API_URL=http://localhost:3000

# URL do frontend (para emails, links, etc)
FRONTEND_URL=http://localhost:3001

# OpenAI (opcional)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-3.5-turbo

# Configurações de email (opcional)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
EMAIL_FROM=noreply@exemplo.com

# Limite de tamanho de upload
UPLOAD_LIMIT=5mb

# Configurações de Admin (usadas em createAdmin.js)
ADMIN_EMAIL=admin@exemplo.com
ADMIN_PASSWORD=senha_segura_aqui
ADMIN_NAME=Administrador

# Configurações do Dyte
DYTE_ORGANIZATION_ID="seu_organization_id"
DYTE_API_KEY="sua_api_key"
DYTE_BASE_URL="https://api.dyte.io/v2"
DYTE_AUTH_HEADER="Basic sua_chave_em_base64"

# Configurações do OpenAI
OPENAI_FALLBACK_MODEL=gpt-3.5-turbo 