# Configurações do Servidor
PORT=3000
CORS_ORIGIN=http://localhost:3001
NODE_ENV=development

# JWT
JWT_SECRET=sua_chave_secreta_aqui
JWT_EXPIRES_IN=8h
JWT_REFRESH_SECRET=sua_chave_refresh_secreta_aqui
JWT_REFRESH_EXPIRES_IN=7d

# Banco de Dados
DATABASE_URL="postgresql://postgres:sistemainfo@localhost:5432/constelacao?schema=public"

# Configurações de Admin (usadas em createAdmin.js)
ADMIN_EMAIL=admin@exemplo.com
ADMIN_PASSWORD=senha_segura_aqui
ADMIN_NAME=Administrador

# Configurações do Dyte
DYTE_ORGANIZATION_ID="seu_organization_id"
DYTE_API_KEY="sua_api_key"
DYTE_BASE_URL="https://api.dyte.io/v2"
DYTE_AUTH_HEADER="Basic sua_chave_em_base64"

# Configurações do OpenAI
OPENAI_API_KEY=sua_chave_da_openai
OPENAI_MODEL=gpt-4o-mini
OPENAI_FALLBACK_MODEL=gpt-3.5-turbo 